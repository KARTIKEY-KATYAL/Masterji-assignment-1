{
    "manifest_version": 3,
    "name": "Screenshot",
    "version": "1.0",
    "description": "A sample Chrome extension to take screen shot and tweet",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "logo.png"
        }
    },
    "icons": {
        "128": "logo.png"
    }
}